{
  "[python]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "charliermarsh.ruff"
  },
  "editor.codeActionsOnSave": {
    "source.fixAll": "explicit",
    "source.organizeImports": "explicit"
  },
  "ruff.codeAction.fixViolation": {
    "enable": true
  },
  "ruff.lint.run": "onSave",
  "cSpell.words": [
    "aiomqtt",
    "arcaea",
    "blurple",
    "collab",
    "dall",
    "daynight",
    "diegetic",
    "dotenv",
    "emeter",
    "emeters",
    "explus",
    "extradata",
    "ffprobe",
    "GPTCMD",
    "Kasa",
    "KHTML",
    "lavfi",
    "leetcode",
    "lowiro",
    "mainstory",
    "maxsplit",
    "mjpeg",
    "moderations",
    "mqtt",
    "mygo",
    "overbrace",
    "palettegen",
    "paletteuse",
    "Referer",
    "sidestory",
    "slst",
    "songlist",
    "tada",
    "vcgencmd",
    "vcodec",
    "vframes",
    "webapi"
  ],
  "taipyStudio.gUI.elementsFilePaths": [],
  "taipyStudio.config.mainPythonFile": "config.py"
}
